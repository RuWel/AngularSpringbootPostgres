docker rmi -f $APP_IMAGENAME
docker build --build-arg git_repo=$APP_GITREPO -t $APP_IMAGENAME .
docker run --network $NETWORK_NAME -e SPRING_DATASOURCE_URL=$SPRING_DATASOURCE_URL -e SPRING_DATASOURCE_USERNAME=$SPRING_DATASOURCE_USERNAME -e \ SPRING_DATASOURCE_PASSWORD=$SPRING_DATASOURCE_PASSWORD -e SPRING_LIQUIBASE_CHANGE_LOG=$SPRING_LIQUIBASE_CHANGE_LOG -e \ SPRING_JPA_PROPERTIES_HIBERNATE_DIALECT=$SPRING_JPA_PROPERTIES_HIBERNATE_DIALECT -e SPRING_JPA_HIBERNATE_DDL_AUTO=$SPRING_JPA_HIBERNATE_DDL_AUTO \
$APP_IMAGENAME
